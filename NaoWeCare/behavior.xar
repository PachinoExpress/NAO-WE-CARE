        self.idPaziente = idPaziente

    def onInput_tipologiaMisurazione(self, tipologia):
        self.tipologia = tipologia

    def onInput_valoreMisurazione(self, valore):
        #'http://192.168.7.100/nao/misurazioni.php'
        url = self.getParameter("URL")
        values = {'cmd' : 'inserisci-misurazione',
                  'id_paziente' : self.idPaziente,
                  'naomarker' : self.naoMarker,
                  'tipologia' : self.tipologia,
                  'valore' : valore }

        try:
            data = urllib.urlencode(values)
            req = urllib2.Request(url, data)
            response = urllib2.urlopen(req)
            jsonData = response.read()

            misurazione = json.loads(jsonData)

            if(misurazione['success']):
                self.risposta( "Misurazione registrata con successo!" )

            else:
                self.risposta("C'è stata un errore durante la registrazione della misurazione")
        except:
                self.risposta("La connessione non è andata a buon fine")

    def onInput_onStop(self):
        self.onUnload()
        self.onStopped()]]>
