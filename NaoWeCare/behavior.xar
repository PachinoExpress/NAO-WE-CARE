import urllib
import json

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self, False)

    def onLoad(self):
        self.idPaziente = -1
        self.naoMarker = -1
        self.tipologia = ""
        self.valore = 0

