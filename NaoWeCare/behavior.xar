import sys
import socket
import time
import urllib2
import urllib
import json

reload(sys)
sys.setdefaultencoding('utf8')

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self, False)

    def onLoad(self):
         self.pippo = 1;

    def onUnload(self):
        self.pippo = 0;

    def onInput_naoMarker(self, naoMarker):
        #'http://192.168.7.100/nao/misurazioni.php'
        self.log("ricezione naoMarker "+ str(naoMarker))

        # Acquisisco dai paramentri del blocco l'indirizzo
        # del server e della pagina a cui connettermi
        url = self.getParameter("URL")

        # Preparo i paramentri POST/GET da inviare al server
        values = {'cmd' : 'ultima-misurazione',
                  'naomarker' : naoMarker }

        # Mi connetto al server ed invio i dati
        data = urllib.urlencode(values)
        req = urllib2.Request(url, data)

        # Aquisisco i dati di risposta della pagina
        response = urllib2.urlopen(req)
        jsonData = response.read()
        self.log(jsonData)

        # Decodifico la stringa ricevuta in un oggetto JSON
        misurazione = json.loads(jsonData)
        self.log(misurazione)

        if(misurazione['success']):
            self.risposta( "L'ultima misurazione di "+ str(misurazione['nome']) +" "+ str(misurazione['cognome']) +" è stata di "+ str(misurazione['tipo']) +" con valore "+ str(misurazione['valore']) )

        else:
            self.risposta("La connessione non è andata a buon fine")

    def onInput_idPaziente(self, idPaziente):
        #'http://192.168.7.100/nao/misurazioni.php'
        self.log("ricezione naoMarker "+ str(idPaziente))

        # Acquisisco dai paramentri del blocco l'indirizzo
        # del server e della pagina a cui connettermi
        url = self.getParameter("URL")

        # Preparo i paramentri POST/GET da inviare al server
        url = self.getParameter("URL")
        values = {'cmd' : 'ultima-misurazione',
                  'id_paziente' : idPaziente }

        # Mi connetto al server ed invio i dati
        data = urllib.urlencode(values)
        req = urllib2.Request(url, data)

        # Aquisisco i dati di risposta della pagina
        response = urllib2.urlopen(req)
        jsonData = response.read()

        # Decodifico la stringa ricevuta in un oggetto JSON
        misurazione = json.loads(jsonData)
        self.log(misurazione)

        if(misurazione['success']):
            self.risposta( "L'ultima misurazione di "+ str(misurazione['nome']) +" "+ str(misurazione['cognome']) +" è stata di "+ str(misurazione['tipo']) +" con valore "+ str(misurazione['valore']) )

        else:
            self.risposta("La connessione non è andata a buon fine")

    def onInput_onStop(self):
        self.onUnload()
        self.onStopped()]]>
